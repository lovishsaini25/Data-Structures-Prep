# https://www.geeksforgeeks.org/problems/factorial5739/1

class Solution:
    def factorial (self, n):
        fact = 1
        for i in range(2, n+1):
            fact = fact * i
        return fact


class Solution:
    def factorial(self, n):
        if n == 0 or n == 1:
            return 1
        return n * self.factorial(n - 1)
